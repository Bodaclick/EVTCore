<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EVT\CoreDomainBundle\Entity\GenericUser" table="generic_user">
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
      <unique-constraint name="email_UNIQUE" columns="email"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="email" type="string" column="email" length="100" nullable="false"/>
    <field name="name" type="string" column="name" length="100" nullable="true"/>
    <field name="surnames" type="string" column="surnames" length="100" nullable="true"/>
    <field name="phone" type="string" column="phone" length="30" nullable="true"/>
    <many-to-many field="provider" target-entity="Provider" mapped-by="genericUser"/>
  </entity>
</doctrine-mapping>
